{
  "title": "Change right_alt to Esc and Control",
  "rules": [
	{
	  "description": "Post cmd if right_alt is tapped",
	  "manipulators": [
        {
    "type": "basic",
    "from": {
        "key_code": "right_alt",
        "modifiers": {
            "optional": [
                "any"
            ]
        }
    },
    "to": [
        {
            "key_code": "left_command"
        }
	  ]
	}
  ]
}
