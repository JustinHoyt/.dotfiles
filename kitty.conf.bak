# font_family
font_family      Hack Nerd Font Mono
italic_font      auto
bold_font        auto
bold_italic_font auto

font_size        16.0

macos_option_as_alt yes
copy_on_select true
enable_audio_bell no

map cmd+[ move_tab_backward
map cmd+] move_tab_forward
map shift+cmd+i set_tab_title

include current-theme.conf


# BEGIN_KITTY_THEME
# One Dark
include current-theme.conf
# END_KITTY_THEME